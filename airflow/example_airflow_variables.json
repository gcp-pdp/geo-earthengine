{
    "base": {
        "destination_dataset_project_id": "gcp-pdp-weather-dev",
        "destination_dataset_name": "geo_weather_dev",
        "export_max_active_runs": 3,
        "output_bucket": "gcp-pdp-weather-dev",
        "output_path_prefix": "export",
        "image_name": "gcr.io/gcp-pdp-weather-dev/geo-exporter",
        "image_version": "1.0.0-64",
        "image_pull_policy": "IfNotPresent",
        "namespace": "default",
        "node_selector": "default-pool",
    },
    "gfs": {
        "destination_table_name": "NOAA_GFS0P25",
        "export_concurrency": 4,
        "export_max_active_runs": 1,
        "export_schedule_interval": "0 4,10,16,22 * * *",
        "export_start_date": "2021-08-25",
        "load_schedule_interval": "10 4,10,16,22 * * *",
        "load_start_date": "2021-08-25",
        "load_concurrency": 4,
        "load_max_active_runs": 1,
        "resources": {
            "request_cpu": "100m",
            "limit_cpu": "500m",
            "request_memory": "64Mi",
            "limit_memory": "256Mi"
        }
    },
    "annual_npp": {
        "destination_table_name": "annual_npp",
        "node_selector": "highmem-node-pool",
        "resources": {
            "request_cpu": "100m",
            "limit_cpu": "2",
            "request_memory": "32Gi",
            "limit_memory": "32Gi"
        }
    },
    "world_pop": {
        "destination_table_name": "world_pop",
        "node_selector": "highmem-node-pool",
        "resources": {
            "request_cpu": "100m",
            "limit_cpu": "3",
            "request_memory": "32Gi",
            "limit_memory": "32Gi"
        }
    }
}